{
	"name": "spotify_connected",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"engines": {
		"node": "16.15.1"
	},
	"cacheDirectory": [
		"node_modules",
		"client/node_modules"
	],
	"scripts": {
		"start": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
		"server": "nodemon index.js",
		"client": "cd client && npm start",
		"postinstall": "cd client && npm i",
		"build": "NODE_ENV=production cd client/ && npm install && npm run build",
		"start-server": "node index.js"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"axios": "^0.27.2",
		"dotenv": "^16.0.1",
		"express": "^4.18.1"
	},
	"devDependencies": {
		"concurrently": "^7.3.0",
		"nodemon": "^2.0.19"
	}
}
